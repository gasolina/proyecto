#include <iostream>
#include <conio.h>

using namespace std;

struct fecha
{int dia;
int mes;
int anho;
};

int compararfechas(fecha f1,fecha f2)
{
	if(f1.anho<f2.anho)
	{
		return 1;
	}
	else
	{
		if(f1.anho>f2.anho)
		{
			return -1;
		}
		else
		{
			if(f1.anho==f2.anho)
			{
				if(f1.mes<f2.mes)
				{
					return 1;
				}
				else
				{
					if(f1.mes>f2.mes)
						return -1;
					else
					{
						if(f1.mes==f2.mes)
						{
							if(f1.dia<f2.dia)
							{
								return 1;
							}
							else
							{
								if(f1.dia>f2.dia)
									return -1;
								else
									return 0;
							}
						}	
					}
				}
			}	
		}
	}
}
					
